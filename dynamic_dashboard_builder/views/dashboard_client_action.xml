<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="action_dashboard_canvas_app" model="ir.actions.client">
    <field name="name">Dashboard</field>
    <field name="tag">dynamic_dashboard_builder.dashboard_canvas_app</field>
  </record>
  <record id="action_dashboard_item_wizard" model="ir.actions.client">
    <field name="name">Dashboard Item Wizard</field>
    <field name="tag">dynamic_dashboard_builder.DashboardItemWizardAction</field>
  </record>

  <menuitem id="menu_dashboard_canvas_root"
            name="Dashboards"
            sequence="10"/>

  <menuitem id="menu_dashboard_canvas"
            name="My Dashboard"
            parent="menu_dashboard_canvas_root"
            action="action_dashboard_canvas_app"
            sequence="1"/>
</odoo>